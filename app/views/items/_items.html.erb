<% if items %>
<div class="row">
    <% items.each do |item| %>
    <div class="row">
        <div class="col-md-3 col-sm-4 col-xs-12">
            <div class="panel pane-default">
                
                <div class="panel-heading text-center">
                    <img src="<%= item.image_url %>" alt="">
                </div>
                <div class="panel panel-body">
                    <p class="item_title"><%= item.name %></p>
                </div>
            </div>
        </div>
    </div>
    <% end %>
</div>
<% end %>